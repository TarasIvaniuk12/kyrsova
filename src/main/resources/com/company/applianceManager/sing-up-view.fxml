<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.applianceManager.SingInController">
   <children>
      <Button defaultButton="true" layoutX="170.0" layoutY="218.0" mnemonicParsing="false" onAction="#singUp" text="Зареєструватися" textAlignment="CENTER" />
      <PasswordField fx:id="password" layoutX="149.0" layoutY="145.0" promptText="Пароль" />
      <TextField fx:id="login" layoutX="149.0" layoutY="109.0" promptText="Логін" />
      <Hyperlink layoutX="229.0" layoutY="353.0" onAction="#changeSceneToSingIn" text="Натисніть увійти" />
      <Label layoutX="123.0" layoutY="356.0" text="Уже маєте акаунт?" />
      <PasswordField fx:id="confirmPassword" layoutX="149.0" layoutY="181.0" promptText="Повторіть пароль" />
      <Button layoutX="400.0" layoutY="14.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
      <Label layoutX="77.0" layoutY="40.0" prefHeight="46.0" prefWidth="294.0" text="Appliance Manager" textAlignment="CENTER">
         <font>
            <Font name="Berlin Sans FB" size="35.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
